{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "ordered-imports": false,
        "indent": [true, "tabs", 4],
        "member-access": false,
        "max-line-length": [true, 180],
        "object-literal-sort-keys": false,
        "no-reference": false
    },
    "rulesDirectory": [],
    "formatters": ["stylish"]
}